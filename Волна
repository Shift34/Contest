using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.IO;

namespace ConsoleApp1
{
    public class Point
    {
        public int X;
        public int Y;

        public Point(int x,int y) 
        {
            this.X = x;
            this.Y = y;
        }


    }

    public class Picks 
    {
        public int count;
        public char symbol;
        public Picks(char symbol)
        {
            count = 0;
            this.symbol = symbol;
        }
    }
    public class Algoritm
    {
        public int count = 0;
        public static int Run(Picks[,] picks)
        {

            return 0;
        }
    }

    class Program
    {
        static void Main(string[] args)
        {
            string line;
            using (StreamReader sr = new StreamReader("input.txt"))
            {
                line = sr.ReadLine();
                string [] input =line.Split();
                Picks[,] picks = new Picks[int.Parse(input[0]), int.Parse(input[1])];
                int numlines = int.Parse(input[0]);
                int numcolumns = int.Parse(input[1]);
                Point point = new Point();
                for(int i=0;i < numcolumns; i++)
                {
                    line = sr.ReadLine();
                    for (int j = 0; j < numlines; j++)
                    {
                        picks[i, j] = new Picks(line[j]);
                    }
                }
            }

        }
    }
}
